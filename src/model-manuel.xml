        <variable name="J_K" units="flux" initial_value="1" interface="public" id="epithelial.J_K"/>
        <variable name="time" units="second" interface="public" id="epithelial.time"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_Na_calculation">
                <eq/>
                <ci>J_Na</ci>
                <apply>
                    <plus/>
                    <ci>J_mc_Na</ci>
                    <ci>G_mc_Na</ci>
                    <apply>
                        <minus/>
                        <ci>J_sc_Na</ci>
                    </apply>
                </apply>
            </apply>
            <apply id="J_Cl_calculation">
                <eq/>
                <ci>J_Cl</ci>
                <apply>
                    <plus/>
                    <ci>2J_mc_Cl</ci>
                    <ci>G_mc_Cl</ci>
                    <ci>G_sc_Cl</ci>
                </apply>
            </apply>
            <apply id="J_K_calculation">
                <eq/>
                <ci>J_K</ci>
                <apply>
                    <plus/>
                    <ci>J_mc_K</ci>
                    <ci>G_mc_K</ci>
                    <ci>J_sc_K</ci>
                    <ci>G_sc_K</ci>
                </apply>
            </apply>
        </math>
    </component>
