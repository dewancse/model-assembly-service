        <variable name="time" units="second" interface="public" id="lumen.time"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="C_m_Na_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>C_m_Na</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>J_mc_Na</ci>
                </apply>
            </apply>
            <apply id="C_m_Cl_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>C_m_Cl</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>2J_mc_Cl</ci>
                </apply>
            </apply>
            <apply id="C_m_K_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>C_m_K</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>J_mc_K</ci>
                </apply>
            </apply>
        </math>
